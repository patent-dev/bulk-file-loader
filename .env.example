# Docker Compose Development Environment
# Copy to .env and customize for your setup

# Docker network name (must exist if external)
DOCKER_NETWORK=reverse-proxy

# Development hostname
DEV_HOST=bulk.l.t

# Traefik entrypoint (websecure for HTTPS, web for HTTP)
TRAEFIK_ENTRYPOINT=websecure

# Frontend API URL
VITE_API_URL=https://bulk.l.t

# Path to client libraries (relative to project root)
CLIENT_LIBS_PATH=../client-libraries
